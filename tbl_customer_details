CREATE TABLE `tbl_customer_details` (
  `CUSTOMER_DETAILS_ID` int NOT NULL,
  `INCOME` decimal(20,2) NOT NULL,
  `DATE_OF_BIRTH` date NOT NULL,
  `WORK_START_DATE` date DEFAULT NULL,
  `IS_MARRIED` tinyint(1) NOT NULL,
  `HOUSE_OWNERSHIP` varchar(100) NOT NULL,
  `CAR_OWNERSHIP` varchar(100) NOT NULL,
  `PROFESSION` varchar(100) NOT NULL,
  `CITY` varchar(100) DEFAULT NULL,
  `STATE` varchar(100) DEFAULT NULL,
  `CURRENT_JOB_START_DATE` varchar(100) DEFAULT NULL,
  `CURRENT_HOUSE_START_DATE` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`CUSTOMER_DETAILS_ID`),
  CONSTRAINT `TBL_CUSTOMER_DETAILS_CUSTOMER_DETAILS_ID_99eaa481_fk_TBL_CUSTO` FOREIGN KEY (`CUSTOMER_DETAILS_ID`) REFERENCES `tbl_customer` (`CUSTOMER_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
