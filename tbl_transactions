CREATE TABLE `tbl_transactions` (
  `TRANSACTION_ID` int NOT NULL AUTO_INCREMENT,
  `SOURCE_ACCOUNT` varchar(18) NOT NULL,
  `DESTINATION_ACCOUNT` varchar(18) NOT NULL,
  `IS_INTERNAL_TRANSACTION` tinyint(1) NOT NULL,
  `TRANSACTION_DATE` date NOT NULL,
  `IS_TRANSACTION_DISPUTED` tinyint(1) NOT NULL,
  `DISPUTE_MESSAGE` varchar(10000) NOT NULL,
  `CUSTOMER_ID` int NOT NULL,
  PRIMARY KEY (`TRANSACTION_ID`),
  KEY `TBL_TRANSACTIONS_CUSTOMER_ID_421a9482_fk` (`CUSTOMER_ID`),
  CONSTRAINT `TBL_TRANSACTIONS_CUSTOMER_ID_421a9482_fk` FOREIGN KEY (`CUSTOMER_ID`) REFERENCES `tbl_customer` (`CUSTOMER_ID`)
) ENGINE=InnoDB AUTO_INCREMENT=2001 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
