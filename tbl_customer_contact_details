CREATE TABLE `tbl_customer_contact_details` (
  `CUSTOMER_CONTACT_DETAILS_ID` int NOT NULL,
  `EMAIL_ID` varchar(150) NOT NULL,
  `PHONE_NUMBER` varchar(10) NOT NULL,
  `ADDRESS` varchar(2000) NOT NULL,
  PRIMARY KEY (`CUSTOMER_CONTACT_DETAILS_ID`),
  CONSTRAINT `TBL_CUSTOMER_CONTACT_DETA_CUSTOMER_CONTACT_DETAILS__64dfbad5_fk` FOREIGN KEY (`CUSTOMER_CONTACT_DETAILS_ID`) REFERENCES `tbl_customer` (`CUSTOMER_ID`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
